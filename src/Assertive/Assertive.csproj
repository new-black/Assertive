<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyTitle>Assertive</AssemblyTitle>
    <AssemblyName>Assertive</AssemblyName>
    <PackageId>Assertive</PackageId>
    <Company>New Black</Company>
    <PackageVersion>0.4.0</PackageVersion>
    <PackageLicenseUrl>https://licenses.nuget.org/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/new-black/Assertive</PackageProjectUrl>
    <RepositoryUrl>https://github.com/new-black/Assertive</RepositoryUrl>
  </PropertyGroup>
  <PropertyGroup>
    <LangVersion>8</LangVersion>
    <Nullable>enable</Nullable>
    <WarningsAsErrors>CS8600;CS8602;CS8603;CS8604;CS8625</WarningsAsErrors>
  </PropertyGroup>
</Project>
